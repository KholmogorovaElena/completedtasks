public class Main {
    public static void main(String[] args) {
        Cat cat = new Cat();
        cat.setName("Жужик");
        System.out.println(cat.getName());
    }
}

class Cat extends Main {
    private String name = "безымянный кот";

    public  void setName(String catName) {
name = catName;

    }
    public String getName () {
        return name;
    }
}
