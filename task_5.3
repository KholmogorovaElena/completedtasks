import java.util.ArrayList;
public class Main {
    public static void main(String[] args) {
ArrayList<Integer> nums1 = new ArrayList<>();
ArrayList<Integer> nums2 = new ArrayList<>();
        for (int i =-26; i<=77; i++) {
            nums1.add(i);
            if (i>0 && i<10) {
                nums2.add(i);
            }
        }
        System.out.println(nums1);
        System.out.println(nums2);
    }
}
