public class Main {

    public static void main(String[] args) {
            String s [] = {"Мама", "Мыла", "Раму"};
            for (int a = 0; a<3; a++)
                    for (int b =0; b<3; b++)
                            if (a!=b)
            System.out.println(s[a]+s[b]+s[3-a-b]);

    }
}
